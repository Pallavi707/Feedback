  <script>
    var ctx = document.getElementById('myChart').getContext('2d');
    var myChart = new Chart(ctx, {
      type: 'line',
      data: {
        labels: ['foodQuality', 'orderAccuracy', 'staffService', 'hygiene', 'foodOptions', 'restaurantEnvironment'],
        datasets: [{
          label: '# of Votes',
          data: [ <%= foodQuality.reduce((a, b) => a + b, 0) %>, <%= orderAccuracy.reduce((a, b) => a + b, 0) %>, 
            <%= staffService.reduce((a, b) => a + b, 0) %>, <%= hygiene.reduce((a, b) => a + b, 0) %>, 
            <%= foodOptions.reduce((a, b) => a + b, 0) %>, <%= restaurantEnvironment.reduce((a, b) => a + b, 0) %>],
          backgroundColor: [
            'rgba(255, 99, 132, 0.2)',
            'rgba(54, 162, 235, 0.2)',
            'rgba(255, 206, 86, 0.2)',
            'rgba(75, 192, 192, 0.2)',
            'rgba(153, 102, 255, 0.2)',
            'rgba(255, 159, 64, 0.2)'
          ],
          borderColor: [
            'rgba(255, 99, 132, 1)',
            'rgba(54, 162, 235, 1)',
            'rgba(255, 206, 86, 1)',
            'rgba(75, 192, 192, 1)',
            'rgba(153, 102, 255, 1)',
            'rgba(255, 159, 64, 1)'
          ],
          borderWidth: 1
        }]
      },
      options: {
        scales: {
          yAxes: [{
            ticks: {
              beginAtZero: true
            }
          }]
        }
      }
    });
  </script>

